{
  "name": "jsyyds",
  "version": "1.1.0",
  "description": "常用JS方法封装",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "🗳️ --- Builds": "",
    "build:cjs": "tsc -p .",
    "build:esm": "tsc -p . --module esnext --outDir dist/esm",
    "build:types": "tsc -p . -d --emitDeclarationOnly --outDir dist/types",
    "🌟 --- Mains": "",
    "build": "rimraf dist && run-p build:*",
    "watch": "rimraf dist && run-p \"build:* -- --watch\"",
    "lint": "eslint src --fix",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "js",
    "function",
    "common",
    "normal",
    "方法封装",
    "js常用",
    "深拷贝",
    "防抖",
    "节流",
    "格式化"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mtiger95/jsyyds/issues"
  },
  "homepage": "https://github.com/mtiger95/jsyyds#readme",
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-typescript": "^7.16.0",
    "@types/webpack-env": "^1.16.3",
    "babel-plugin-import": "^1.13.3",
    "husky": "^7.0.4",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "conventional-changelog-conventionalcommits": "^4.6.1",
    "eslint": "^7.x.x",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "lint-staged": "^12.1.2",
    "microbundle": "^0.14.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "semantic-release": "^18.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mtiger95/jsyyds.git"
  }
}
